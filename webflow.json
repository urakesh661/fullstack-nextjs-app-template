{
  "cosmic": {
    "framework": {
      "type": "nextjs",
      "config": {
        "version": "15.0.0",
        "basePath": "/apppmm",
        "assetPrefix": "/apppmm"
      }
    },
    "build": {
      "install_command": "npm install",
      "build_command": "npm run build",
      "output_directory": ".next"
    },
    "dev": {
      "command": "npm run dev",
      "port": 3000
    },
    "env": {
      "NODE_ENV": "production"
    }
  }
}
